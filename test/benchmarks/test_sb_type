/*
	add x1, x0, x0      # 0
    add x8, x0, x0      # 4
    addi x2, x0, 10     # 8
    addi x3, x0, 2      # 12
	addi x4, x0, 2      # 16
    addi x5, x0, 1024   # 20
    addi x5, x5, 1024   # 24

    INCR:
    addi x1 , x1, 1     # 28
    mul x4, x4, x3      # 32
    div x5, x5, x3      # 36
    bne x1 , x2, INCR   # 40
    
    jal x7, NEXT       # 44
    addi x7, x0, -500  # 48
    
    NEXT:
    beq x1, x2, DECR    # 52
    jal x8, NEXT       # 56
	  
    DECR:
    addi x1, x1, -1     # 60
    blt x0, x1, DECR    # 64
	 
	 END:
	 jal x6, END       # 68
*/

