/* 
   Simple arithmetic benchmark testing add, addi, and sub.
   Expected results: x1 = -4, x2 = -4, x3 = 4, x4 = -4, x5 = -10, x6 = 0, x7 = -6
*/

/*
  add x1, x0, x0  # x1 = 0
  addi x1, x1, 5  # x1 = 5
  addi x1, x1, -7  # x1 = -2
  add x1, x1, x1  # x1 = -4
  add x2, x0, x1  # x2 = -4
  sub x3, x0, x1  # x3 = 4
  sub x4, x1, x0  # x4 = -4
  addi x5, x4, -6 # x5 = -10
  sub x6, x3, x3  # x6 = 0
  sub x7, x5, x2  # x7 = -6
  NOP x3
*/

